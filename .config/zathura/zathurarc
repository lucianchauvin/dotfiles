# textcolor
set default-bg \#282828
set completion-bg \#242021
set highlight-color \#e68183
set highlight-active-color \#e68183
set inputbar-bg \#282828
set inputbar-fg \#ebdbb2

set completion-highlight-bg \#e6d6ac
set statusbar-bg \#282828
set statusbar-fg \#ebdbb2
set index-bg \#2a2426
set index-fg \#e6d6ac
set statusbar-home-tilde true
set index-active-bg \#e68183
set font 'NotoSansM Nerd Font Mono 11'
set font size Normal

set selection-clipboard clipboard
set vertical-center
